{
    "extends": "astro/tsconfigs/base",
    "compilerOptions": {
      "strictNullChecks": true,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"],
        "@utils/*": ["src/utils/*"],
        "@i18n/*": ["src/i18n/*"],
        "@assets/*": ["src/assets/*"],
        "@components/*": ["src/components/*"],
        "@styles/*": ["src/styles/*"],
        "@layouts/*": ["src/layouts/*"]
      }
    }
  }
